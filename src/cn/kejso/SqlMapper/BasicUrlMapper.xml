<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SqlMapper.BasicUrlMapper">

	<resultMap type="BasicUrl"  id="BasicUrlMap">
		<id property="id" column="id" />
		<result property="title" column="title" />
		<result property="url" column="url" />
	</resultMap>
	
	
	<insert id="insertBasicUrl" parameterType="BasicUrl" useGeneratedKeys="true" keyProperty="id">
    	 insert into wanfangurl (title,url)
    	 		values(#{title},#{url})
    </insert>
    
	<select id="getAllBasicUrl" resultMap="BasicUrlMap">
	     select *
	     from  wanfangurl where id  &gt;  129245
	</select>
	
	<select id="getPartBasicUrl" resultMap="BasicUrlMap">
	     select *
	     from  wanfangurl where id  &gt;  116913 &amp;&amp; id &lt; 118062
	</select>
	
</mapper>